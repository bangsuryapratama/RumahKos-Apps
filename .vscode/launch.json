{
 "version": "0.2.0",
 "configurations": [
  {
   "name": "Attach to Chrome",
   "port": 9222,
   "request": "attach",
   "type": "chrome",
   "webRoot": "${workspaceFolder}"
  },

  {
   "name": "rumahkosapps",
   "request": "launch",
   "type": "dart"
  },
  {
   "name": "rumahkosapps",
   "request": "launch",
   "type": "dart",
    "args" :[
     "web-port",
     "5000"
    ]
  },
  {
   "name": "rumahkosapps (release mode)",
   "request": "launch",
   "type": "dart",
   "flutterMode": "release"
  },
  {
   "name": "rumahkosapps (Flutter)",
   "program": "lib/main.dart",
   "request": "launch",
   "type": "dart"
  },

  {
   "name": "rumahkosapps (Chrome)",
   "request": "launch",
   "type": "dart",
   "deviceId": "chrome"
  }

 ]
}
